        if (Ma_so_Xu_ly == "KHOI_DONG_DU_LIEU_PHAN_HE_NHAN_VIEN")
        {
            var Du_lieu_Kq = Du_lieu.Tao_Du_lieu_Phan_he_Nhan_vien();
